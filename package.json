    "@vueuse/core": "^6.9.2",
    "date-fns": "^2.27.0",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.6",
    "qs": "^6.10.2",
    "umi-request": "^1.4.0",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
